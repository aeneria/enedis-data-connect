{
    "name": "aeneria/enedis-data-connect",
    "description": "A small library to use a part of Enedis Data Connect API with PHP : https://datahub-enedis.fr/data-connect/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Mellerin",
            "email": "bonjour@aeneria.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "symfony/http-client": "5.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.5",
        "symfony/serializer-pack": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Aeneria\\EnedisDataConnectApi\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aeneria\\EnedisDataConnectApi\\Tests\\": "tests/"
        }
    }
}
